@charset "utf-8";
/* CSS Document */

@font-face{
	font-family:Royalacid;
	src:url(../fontes/Royalacid.ttf);
}

*{
	text-decoration:none;
	Padding:0;
	margin:0;
}

body, html{
	height:100%;
	background:url(../IMG/fundo.png) ;
}

#interface{
	min-height: 75%;
}

#banner{
	display: block;
	width: 100%;
	height: 400px;
}
/*MENU*/

#logo-menu{
	width:10%;
	margin: 10px 0 0 10%;
	position:relative;
	display: inline-block;
	height:120px;
}

#logo-menu img{
	height:100%;
	width:100%;
}

#logo-menu:hover{
	transform: rotatey(360deg);
	transition: 6s;
}

#container-menu {
	position: relative;
	font-family:Royalacid;
	height:140px;
	background-color:#000;

}

#container-menu a{
	text-decoration: none;
}
/*BARRA DE NAVEGAÇÃO*/
#container_menu_loja{
	background:#2E2E2E;
	position: relative;
}
#menu-loja{
	padding: 10px 0 0 0;
	display: table;
	margin: auto;
	position: relative;
}

#menu-loja li{
	position: relative;
	display: inline-block;
	font-size: 24px;
}

#menu-loja li p{
	padding: 0px 20px;
	border-left: 2px solid #818181;
	text-decoration: none;
	color: #818181;
	transition: 1s;
}

.nomecat{
	text-decoration: none;
	color: #818181;
	padding:10px;
	transition: 1s;
	display: block
}

#menu-loja li a{
	text-decoration: none;
	color: #818181;
}

#menu-loja ul li ul{
	margin:auto;
	text-align: center;
	width: 150px;
	position: absolute;
	background: #000;
	display: none;
	list-style: none;
	box-shadow: 4px 4px 3px 0px #818181;
	z-index: 1;
	left:0;

}

#menu-loja ul li ul a p{
	Color: #818181;
	padding:10px;
}

#menu-loja li:hover ul {
	transform: translateY(0%);
	display: block;
	transition-delay: 0s, 0s, 0.3s;
}

#menu-loja ul li ul a span:hover {
	transition: 0.5s;
	color:#000;
	background:#9a9a9a;
}

#menu-loja li:hover a{
	color:#dbdbdb;
}

/*MENU DE BUSCA*/

#menu-busca{
	width: 35%;
	display: inline-block;
	z-index: 1;
	position: relative;
	left: 11%;
	top: -50px;
}

#busca{
	width: 100%;
	height: 30px;
	display: block;
	-webkit-margin-start: 2px;
	-webkit-margin-end: 2px;
	-webkit-padding-before: 3px;
	-webkit-padding-start: 3px;
	-webkit-padding-end: 3px;
	-webkit-padding-after: 3px;
	min-width: -webkit-min-content;
	border-width: 2px;
	border-style: groove;
	border-color: #818181;
	border-image: initial;
}

#texto-busca{
	font-size: 20px;
	width: 100%;
	height: 100%;
	color:#818181;
	border:0;
	background:transparent;
}

#texto-busca:focus{
	border:0;
	box-shadow: 0 0 0 0;
	outline: 0;
	cursor:auto;
}

#botao-busca {
	font-size: 20px;
	width: 7%;
	height: 39px;
	position: absolute;
	left: 102.5%;
	top: 1px;
}

/*MENU DO CLIENTE Deslogado*/

#menu_cliente {
	top: -40px;
	max-width: 10%;
	display: inline-block;
	position: relative;
	transition: 1s;
	left: 30%;
}

#img_menu_cliente{
	background: url(../img/Perfil.png) top no-repeat;
	width: 40px;
	height: 40px;
	position: relative;
	background-size: 40px;
	float: left;
}

#menu_cliente a p{
	min-width: 50px;
	font-size: 19px;
	color: #818181;
	text-align: center;
	text-decoration: none;
	display: inline-block;
	position: relative;
	top: 20px;
	float: left;
	margin-right: 5px;
}

#menu_cliente:hover a p{
	transition: 0.5s;
	color:#dbdbdb;
	box-shadow: 0px 0px 1 px #dbdbdb;
}

#menu_cliente:hover  #img_menu_cliente{
	transition: 0.2s;
	background:url(../img/Perfil2.png) top no-repeat;
	background-size: 40px;
}

/**Menu Cliente Logado**/
#userlog {
	top: -70px;
	max-width: 10%;
	display: inline-block;
	position: relative;
	transition: 1s;
	left: 30%;
}

#userlog p {
	min-width: 50px;
	font-size: 19px;
	color: #818181;
	text-align: center;
	text-decoration: none;
	display: inline-block;
	position: relative;
	top: 20px;
	float: left;
	margin-right: 5px;
}

#img_menuclientedrop{
	background: url(../img/Setacliente.png);
	width: 50px;
	height: 50px;
	float: left;
	position: relative;
	text-align: center;
	margin: 0px 30px 0px 30px;
}

#img_userlog{
	background: url(../img/Perfil.png) top no-repeat;
	width: 40px;
	height: 40px;
	position: relative;
	background-size: 40px;
	float: left;
}

#userlog ul{
	text-align: center;
	width: 200px;
	position: absolute;
	top: 90px;
	background: #000;
	display: none;
	list-style: none;
	box-shadow: 4px 4px 3px 0px #818181;
	z-index: 1;
	left: -50px;
}

#userlog ul a{
	font-size: 19px;
	color:#818181;
	text-decoration:none;
	margin-right: 5px;
}

#userlog:hover ul {
	display: block;
}

